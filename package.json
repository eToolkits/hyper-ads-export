{
  "name": "hyper-ads-export",
  "author": "hungnm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.17",
    "@chakra-ui/react": "^1.6.12",
    "@electron/remote": "^1.0.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@lottiefiles/react-lottie-player": "^3.4.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "electron-is-dev": "^1.2.0",
    "firebase": "^9.5.0",
    "formik": "^2.2.9",
    "framer-motion": "^4",
    "iconsax-react": "^0.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.4.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.1",
    "redux": "^4.1.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "package": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "wait-on": "^5.2.1"
  },
  "husky": {
    "hook": {
      "pre-commit": "yarn lint"
    }
  }
}
